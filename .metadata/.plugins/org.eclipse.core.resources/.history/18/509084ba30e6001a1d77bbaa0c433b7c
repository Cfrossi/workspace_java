package userinterface;

import CartaoCash.CartaoPrePago;

public class Banco {
	public static void main(String args[]) {
		CartaoPrePago cartao1;
		
		cartao1 = new CartaoPrePago("123", "Caio", 2021, 10, 1000.0);
		
		System.out.println(cartao1);
		
		cartao1.adicionarCredito(500);
		System.out.println(cartao1);
		
		
	}

}
